{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "exclude": ["./node_modules"],
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "types": ["node", "svelte"]
  }
}
